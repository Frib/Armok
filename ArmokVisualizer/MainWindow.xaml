<Window x:Class="ArmokVisualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="342" Width="725">
    <Grid>
        <TextBox Height="225" HorizontalAlignment="Left" Margin="12,12,0,0" Name="program" VerticalAlignment="Top" Width="298" AcceptsReturn="True" AcceptsTab="True" DataContext="{Binding}" TextWrapping="Wrap" />
        <TextBox Height="254" HorizontalAlignment="Left" Margin="316,12,0,0" Name="uristinfo" VerticalAlignment="Top" Width="375" AcceptsReturn="True" AcceptsTab="True" />
        <Button Content="Start" Height="23" HorizontalAlignment="Left" Margin="231,268,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,243,0,0" Name="input" VerticalAlignment="Top" Width="298" AcceptsReturn="True" AcceptsTab="True" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,268,0,0" Name="comboBox1" VerticalAlignment="Top" Width="104" />
        <Button Content="Debug" Height="23" HorizontalAlignment="Left" Margin="150,268,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
    </Grid>
</Window>
